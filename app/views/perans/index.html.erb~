  <thead>
    <tr>
      <th>Peran</th>
      <th>Jabatan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= @perans.inspect %>
    <% @perans.each do |peran| %>
      <tr>
        <td><%= peran.peran %></td>
        <td><%= peran.jabatan %></td>
        <td><%= link_to 'Show', peran %></td>
        <td><%= link_to 'Edit', edit_peran_path(peran) %></td>
        <td><%= link_to 'Destroy', peran, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

